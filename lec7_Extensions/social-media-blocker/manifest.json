{
    "name": "Social Media Blocker",
    "version": "1.0",
    "description": "It blocks the websites on the list!",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "hello.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "tabs"
    ]
}